{
  "name": "angular2-froala-wysiwyg-base",
  "version": "2.5.0",
  "description": "Angular2 bindings for Froala WYSIWYG HTML rich text editor",
  "main": "bundles/angular2-froala-wysiwyg.umd.js",
  "typings": "index.d.ts",
  "module": "index.js",
  "scripts": {
    "lite-server": "lite-server -c demo/bs-config.json",
    "demo.serve": "run-s build link demo.build lite-server",
    "demo.build": "ng build -prod --aot",
    "build": "ngm build -p src --clean",
    "link": "ngm link -p src --here",
    "build.watch": "ngm build -p src --watch --skip-bundles",
    "start": "ng serve --aot --host 0.0.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/froala/angular2-froala-wysiwyg.git"
  },
  "keywords": [
    "angular2",
    "froala",
    "html",
    "text",
    "editor",
    "wysisyg",
    "jquery-plugin",
    "rich editor",
    "rich text editor",
    "rte",
    "javascript",
    "jquery"
  ],
  "author": "Froala Labs (https://www.froala.com/)",
  "license": "https://froala.com/wysiwyg-editor/terms/",
  "bugs": {
    "url": "https://github.com/froala/angular2-froala-wysiwyg/issues"
  },
  "dependencies": {
    "froala-editor": "2.4.2"
  },
  "peerDependencies": {
    "@angular/common": "^2.3.1",
    "@angular/compiler": "^2.3.1",
    "@angular/core": "^2.3.1",
    "@angular/forms": "^2.3.1"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0-beta.29",
    "@angular/common": "^2.4.4",
    "@angular/compiler": "^2.4.4",
    "@angular/compiler-cli": "^2.4.4",
    "@angular/core": "^2.4.4",
    "@angular/forms": "^2.4.4",
    "@angular/http": "^2.4.4",
    "@angular/language-service": "^2.4.4",
    "@angular/platform-browser": "^2.4.4",
    "@angular/platform-browser-dynamic": "^2.4.4",
    "@angular/router": "^3.4.4",
    "@angular/tsc-wrapped": "0.5.1",
    "@ngtools/webpack": "1.2.7",
    "@types/jquery": "^2.0.40",
    "@types/marked": "0.0.28",
    "@types/node": "7.0.0",
    "@types/webpack": "^2.2.2",
    "classlist-polyfill": "1.0.3",
    "ngm-cli": "0.4.4",
    "ts-helpers": "^1.1.1",
    "typescript": "2.0.10"
  }
}
